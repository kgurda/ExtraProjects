<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_start(form)}}
    {{ form_errors(form) }}

    Imię i Nazwisko {{ form_widget(form.name) }} <br>
    Email {{ form_widget(form.email) }} <br>
    Login {{ form_widget(form.username) }} <br>
    Hasło {{ form_widget(form.plainPassword.first) }} <br>
    Powtórz hasło {{ form_widget(form.plainPassword.second) }}
 
    {{ form_rest(form) }}
    <input type="submit" value="{{ 'Zarejestruj'|trans({}, 'FOSUserBundle') }}" />
</form>